<div class="page-content">
            

            <div class="page-header">
              <h1>
                Event Categories Management
                <small>
                  <i class="ace-icon fa fa-angle-double-right"></i>
                  
                </small>
              </h1>
            </div><!-- /.page-header -->

            <div class="row">
              <div class="col-xs-12">
                <!-- PAGE CONTENT BEGINS -->
                <!-- /.row -->
                <div class="clearfix">
        <div class="pull-left alert alert-success no-margin alert-dismissable">
          <button data-dismiss="alert" class="close" type="button">
            <i class="ace-icon fa fa-times"></i>
          </button>

          <i class="ace-icon fa fa-umbrella bigger-120 blue"></i>
          Click on the 'New' button to add new event category...
        </div>
        <div class="pull-right">
          <a class="btn btn-primary no-radius" href="<%=new_admin_event_category_path%>" data-method="GET">
            <i class="ace-icon fa fa-pencil-square-o bigger-120"></i>New
          </a>
        </div>
      </div>

      <div class="hr dotted"></div>

                <div class="row">
                  <div class="col-xs-12">
                    

                    <div class="clearfix">
                      
                    </div>
                    <div class="table-header">
                      Results for "Event Categories"
                    </div>

                    <!-- div.table-responsive -->

                    <!-- div.dataTables_borderWrap -->
                    <div>
                      <div id="dynamic-table_wrapper" class="dataTables_wrapper form-inline no-footer">
                        <div class="row">
                          <div class="col-xs-6">
                          </div>
                          <div class="col-xs-6">
                          <%= form_tag @search, url: admin_event_categories_path , method: :get, remote: true do %>
                            <div id="dynamic-table_filter" class="dataTables_filter">
                              <label>Search:<%= search_field_tag :data,nil, class:"form-control input-sm", id:"search_event_categories"%>
                              <!-- <input type="search" class="form-control input-sm" placeholder="" aria-controls="dynamic-table"> -->
                              </label>
                            </div>
                            <% end %>

                          </div>
                        </div>

                        <!-- start -->
                        <table class="table table-striped table-bordered table-hover dataTable no-footer" id="dynamic-table" role="grid" aria-describedby="dynamic-table_info">
                        <thead>
                          <tr role="row">
                            <th class="" tabindex="0" aria-controls="dynamic-table" rowspan="1" colspan="1" aria-label="Domain: activate to sort column ascending">Name</th>

                            <th class="hidden-480 " tabindex="0" aria-controls="dynamic-table" rowspan="1" colspan="1" aria-label="Clicks: activate to sort column ascending">Color Code</th>

                            <th class="hidden-480 " tabindex="0" aria-controls="dynamic-table" rowspan="1" colspan="1" aria-label="Status: activate to sort column ascending">Actions</th>
                          </tr>
                        </thead>
                        <tbody id="tbody_event_categories">
                       <%=render partial: 'admin/event_categories/event_categories_index', :locals => {event_categories: @event_categories}%>
                          </tbody>
                      </table>

                        <!-- end -->


                      <div class="row">
                      <div class="col-xs-6">
                      </div>
                      <div class="col-xs-6">
                      <div class="dataTables_paginate paging_simple_numbers" id="dynamic-table_paginate">
                      <%= will_paginate @event_categories %>
                      </div>
                      </div>
                      </div>
                      </div>
                    </div>
                  </div>
                </div>

              

                <!-- PAGE CONTENT ENDS -->
              </div><!-- /.col -->
            </div><!-- /.row -->
          </div>
